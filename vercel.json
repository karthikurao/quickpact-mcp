{
  "version": 2,
  "builds": [
    {
      "src": "api/mcp.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/mcp/(.*)",
      "dest": "/api/mcp.py"
    },
    {
      "src": "/",
      "dest": "/api/mcp.py"
    }
  ],
  "env": {
    "QUICKPACT_AUTH_TOKEN": "quickpact_supersecret_token_2025",
    "MY_NUMBER": "919876543210"
  }
}
